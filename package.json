{
  "name": "streaming-studio",
  "version": "0.0.1",
  "description": "A streaming application by electron.",
  "main": "dist/app.js",
  "repository": "https://github.com/toro-ponz/streaming-studio.git",
  "author": "toro_ponz <toro-ponz@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "start": "electron ./dist/app.js",
    "build": "tsc && webpack",
    "build:main": "tsc",
    "build:render": "webpack",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "package": "tsc && webpack && electron-builder --win --x64"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@types/styled-components": "^5.1.7",
    "eslint-loader": "^4.0.2",
    "html-webpack-plugin": "^5.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-redux": "^7.1.16",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "css-loader": "^5.1.0",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "eslint": "^7.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "prettier": "^2.2.1",
    "scss-loader": "^0.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0"
  },
  "build": {
    "appId": "com.toroponz.studio",
    "productName": "StreamingStudio",
    "files": [
      "assets/**/*",
      "dist/**/*",
      "package.json"
    ]
  }
